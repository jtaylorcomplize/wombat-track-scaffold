+  /**
+   * Extract meaningful identifier from Notion URL
+   */
+  private extractNotionId(notionUrl: string): string {
+    // Extract the readable part before the hash
+    const match = notionUrl.match(/\/([^\/\?]+)-[a-f0-9]{32}/);
+    if (match) {
+      return match[1].replace(/-/g, ' ');
+    }
+    return '';
+  }
