{
  "name": "wombat-track",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "admin-server": "tsx src/server/admin-server.ts",
    "dev:full": "concurrently --names \"Frontend,Backend,Admin\" --prefix-colors \"cyan,yellow,magenta\" \"npm run dev\" \"npm run server\" \"npm run admin-server\"",
    "dev:backend": "concurrently --names \"Backend,Admin\" --prefix-colors \"yellow,magenta\" \"npm run server\" \"npm run admin-server\"",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:ui": "jest tests/ui",
    "test:ui:headed": "HEADLESS=false jest tests/ui",
    "test:ui:watch": "jest tests/ui --watch",
    "test:admin:sidebar": "jest tests/admin-ui-sidebar-table.test.js --detectOpenHandles --forceExit",
    "qa:admin-ui-phase1": "jest tests/admin-ui-qa/admin-ui-qa-suite.test.js --detectOpenHandles --forceExit --verbose",
    "qa:subapp-phase5": "jest tests/subapp-qa/subapp-qa-phase5.test.js --detectOpenHandles --forceExit --verbose",
    "sdlc:check-commit": "scripts/check-commit-message.sh",
    "sdlc:test-dispatchers": "node .github/scripts/test-dispatchers.cjs",
    "sdlc:watchdog": "node .github/scripts/sidebar-watchdog.cjs",
    "sdlc:validate": "npm run lint && npm run test && npm run sdlc:test-dispatchers && npm run sdlc:watchdog",
    "qa:framework": "node scripts/qa-framework/run-qa-framework.js",
    "qa:framework:staging": "node scripts/qa-framework/run-qa-framework.js --environment staging",
    "qa:framework:dev": "node scripts/qa-framework/run-qa-framework.js --environment development",
    "qa:admin-ui": "node scripts/qa-framework/enhanced-admin-qa.js",
    "qa:configurable": "node scripts/qa-framework/configurable-qa-framework.js",
    "of-8.5:init": "tsx scripts/initialize-of-8.5.ts",
    "of-8.5:migrate": "tsx scripts/db-migration-of-8.5.ts migrate",
    "of-8.5:rollback": "tsx scripts/db-migration-of-8.5.ts rollback",
    "of-8.5:verify": "tsx scripts/db-migration-of-8.5.ts verify",
    "uat:nightly": "tsx scripts/nightly-uat-automation.ts run",
    "uat:cloud-validation": "jest tests/of-8.5-cloud-runtime-validation.spec.js --testTimeout=120000 --detectOpenHandles --forceExit",
    "mcp:install": "cd config/mcp-mssql && npm install && cd ../mcp-azure && npm install",
    "mcp:build": "tsc -p config/mcp-mssql/tsconfig.json && tsc -p config/mcp-azure/tsconfig.json",
    "start:mcp-mssql": "tsx src/services/mcpMSSQLServer.ts",
    "start:mcp-azure": "tsx src/services/mcpAzureServer.ts",
    "start:event-orchestrator": "tsx src/services/eventDrivenOrchestrator.ts",
    "mcp:dev": "concurrently --names \"MSSQL,Azure,Events\" --prefix-colors \"blue,green,yellow\" \"npm run start:mcp-mssql\" \"npm run start:mcp-azure\" \"npm run start:event-orchestrator\"",
    "mcp:health": "curl -f http://localhost:8002/health && curl -f http://localhost:8003/health",
    "mcp:docker-build": "docker-compose -f config/mcp-mssql/docker-compose.yml build",
    "mcp:docker-up": "docker-compose -f config/mcp-mssql/docker-compose.yml up -d",
    "mcp:docker-down": "docker-compose -f config/mcp-mssql/docker-compose.yml down",
    "mcp:docker-logs": "docker-compose -f config/mcp-mssql/docker-compose.yml logs -f",
    "azure:provision": "scripts/azure-openai-provision.sh",
    "azure:validate": "az account show && az cognitiveservices account list",
    "of-8.6:init": "npm run azure:provision && npm run mcp:docker-build",
    "of-8.6:deploy": "npm run mcp:docker-up && sleep 30 && npm run mcp:health",
    "of-8.6:test": "npm run uat:cloud-validation && npm run mcp:health",
    "of-8.6:migrate-jsonl": "tsx scripts/migrate-jsonl-to-canonical.ts",
    "of-8.6:validate-compliance": "tsx scripts/validate-au-compliance.ts",
    "of-8.6:full-deployment": "npm run of-8.6:init && npm run of-8.6:deploy && npm run of-8.6:migrate-jsonl && npm run of-8.6:test",
    "build:check": "tsc --noEmit",
    "test:oes": "tsx scripts/test-orchestrator-execution.ts",
    "validate:governance": "tsx scripts/oes-governance-validation.ts",
    "test:protocol": "scripts/oes-testing-protocol.sh"
  },
  "dependencies": {
    "@azure/core-auth": "^1.10.0",
    "@azure/identity": "^4.11.1",
    "@azure/keyvault-secrets": "^4.10.0",
    "@azure/openai": "^2.0.0",
    "@looker/embed-sdk": "^2.0.6",
    "@notionhq/client": "^4.0.1",
    "@tiptap/extension-placeholder": "^3.0.7",
    "@tiptap/extension-typography": "^3.0.7",
    "@tiptap/react": "^3.0.7",
    "@tiptap/starter-kit": "^3.0.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.10.0",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.4.4",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.12.0",
    "puppeteer": "^24.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^8.14.1",
    "@octokit/rest": "^19.0.13",
    "uuid": "^9.0.0",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.32.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^4.0.0",
    "typescript-eslint": "^8.37.0",
    "vite": "^5.4.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/uuid": "^9.0.2",
    "@types/node-fetch": "^3.0.3"
  }
}
