{
  "name": "mcp-mssql-server",
  "version": "1.0.0",
  "description": "MCP Server for MSSQL canonical cloud database integration - OF-8.6",
  "main": "dist/mcpMSSQLServer.js",
  "type": "module",
  "scripts": {
    "start": "node dist/mcpMSSQLServer.js",
    "dev": "nodemon --exec tsx src/services/mcpMSSQLServer.ts",
    "build": "tsc -p tsconfig.mcp.json",
    "test": "jest --config jest.mcp.config.js",
    "health": "curl -f http://localhost:8002/health",
    "logs": "docker logs wt-mcp-mssql-server -f"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mssql": "^10.0.2",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-beta.20",
    "redis": "^4.6.12",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "prometheus-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/mssql": "^9.1.4",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
